<Page x:Class="CleaningCompany.ViborKassir"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CleaningCompany"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ViborKassir">

    <Grid Background="AliceBlue">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.3*"/>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="0.3*"/>
            <RowDefinition Height="0.3*"/>
            <RowDefinition Height="0.3*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="0.05*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="PageFrameVibor2" Grid.ColumnSpan="5" Grid.RowSpan="5" Grid.Column="0"  Grid.Row="1" NavigationUIVisibility="Hidden"/>
        <ComboBox x:Name="Vibor" Grid.Column="1" Grid.ColumnSpan="3" Grid.Row="0" SelectionChanged="Vibor_SelectionChanged"/>
        <Button x:Name="Auth" Content="Вернуться к авторизации" Click="Auth_Click"/>
    </Grid>
</Page>
